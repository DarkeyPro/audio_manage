	_on_MenuButton_item_selected($VBoxContainer/MenuButton.selected )


func _on_nextsub2_pressed():
	$VBoxContainer/MenuButton2.selected +=1
	pass # Replace with function body.


func _on_nextsub_pressed():
	$VBoxContainer/MenuButton2.selected -=1
	pass # Replace with function body.



func _on_play_sub_pressed():
	Audio_Manager._play(0,$VBoxContainer/MenuButton2.selected,true)
	pass # Replace with function body.


func _on_MenuButton_item_selected(ID):
	$VBoxContainer/MenuButton2.items = []
	for i in Audio_Manager.sub_tracks[$VBoxContainer/MenuButton.selected]:
		$VBoxContainer/MenuButton2.add_item(i.resource_path)
	pass # Replace with function body.

